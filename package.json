{
  "name": "provision-by-claim",
  "description": "Using Lambda function to register device in Provision by Claim",
  "version": "0.0.0-development",
  "type": "module",
  "scripts": {
    "test": "jest",
    "prepare": "husky install && check-node-version --package",
    "deploy": "cdk deploy",
    "synth": "cdk synth"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pudkrong/provision-by-claim.git"
  },
  "bugs": {
    "url": "https://github.com/pudkrong/provision-by-claim/issues"
  },
  "homepage": "https://github.com/pudkrong/provision-by-claim#readme",
  "keywords": [
    "nordicsemiconductor",
    "fleet",
    "provision",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-lambda-powertools/logger": "1.12.1",
    "@aws-sdk/client-iam": "3.386.0",
    "@aws-sdk/client-iot": "3.386.0",
    "@aws-sdk/client-ssm": "3.386.0",
    "@aws-sdk/client-sts": "3.386.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.69",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "16.0.12",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.75",
    "@swc/jest": "0.2.28",
    "@types/aws-lambda": "8.10.119",
    "@types/jest": "29.5.3",
    "@types/yazl": "2.4.2",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "aws-cdk": "2.90.0",
    "aws-cdk-lib": "2.90.0",
    "cdk": "2.90.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "commander": "11.0.0",
    "constructs": "10.2.69",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "48.0.1",
    "glob": "10.3.3",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "lint-staged": "13.2.3",
    "mqtt": "5.0.2",
    "prettier": "3.0.1",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@middy/core": "4.5.5",
    "@nordicsemiconductor/from-env": "2.0.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "lodash_es"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "$schema": "https://json.schemastore.org/swcrc",
          "jsc": {
            "parser": {
              "syntax": "typescript"
            }
          }
        }
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
